! Mouse
Xcursor.size: 48

! XFT
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.autohint: false
Xft.rgba: rgb
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

!behavior
URxvt.termName: rxvt-unicode-256color
URxvt.loginShell: true
URxvt.scrollBar: false
URxvt.secondaryScroll: true
URxvt.saveLines: 65535
URxvt.cursorUnderline: true 
URxvt.urgentOnBell: true
URxvt.cursorBlink: false
URxvt.cutchars: '"'().<>[]{|}
Urxvt.urlLauncher: /usr/bin/firefox

!extensions
!URxvt.perl-lib: ~/.config/urxvt/perl
urxvt.perl-ext-common: resize-font

!keys
URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show

! look
URxvt.font: xft:Hack Nerd Font Mono:size=10
URxvt.boldFont: xft:Hack Nerd Font Mono:size=10:Bold
! URxvt.font: xft:Anonymice Nerd Font:size=10
! URxvt.boldFont: xft:Anonymice Nerd Font:size=10:Bold
URxvt.letterSpace: -2
URxvt.matcher.button: 1
URxvt.geometry: 70x35
URxvt.internalBorder: 1 
URxvt.inheritPixmap: True
URxvt.foreground:   #bea492
URxvt.depth: 32
! URxvt.background:   rgba:0000/0000/0200/c800   
URxvt.background:   Black
URxvt.cursorColor:  #bea492
URxvt.colorBD:      #bea492


